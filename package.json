{
  "name": "MS-proxy",
  "version": "0.1.7a",
  "description": "A Minecraft 1.8.9 proxy for Hypixel with advanced plugin support",
  "main": "main.js",
  "scripts": {
    "start": "chcp 65001 && electron .",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.MS.proxy",
    "productName": "MS-Proxy",
    "win": {
      "target": "portable",
      "icon": "icon.png"
    },
    "extraResources": [
      "MS-PROXY_OPTIMIZATION_TOOL.bat"
    ],
    "portable": {
      "artifactName": "MS-Proxy.exe",
      "requestExecutionLevel": "user"
    },
    "files": [
      "**/*",
      "!dist/*",
      "!auth_cache/*",
      "!data/*",
      "!config/*",
      "!plugins/*"
    ]
  },
  "keywords": [
    "minecraft",
    "proxy",
    "protocol"
  ],
  "author": "ByMS",
  "license": "UNLICENSED",
  "dependencies": {
    "adm-zip": "^0.5.16",
    "axios": "^1.13.2",
    "commander": "^14.0.0",
    "express": "^5.2.1",
    "form-data": "^4.0.5",
    "keytar": "^7.9.0",
    "lodash": "^4.17.21",
    "minecraft-protocol": "^1.57.0",
    "node-forge": "^1.3.1",
    "node-machine-id": "^1.1.12",
    "prismarine-auth": "^2.7.0",
    "socket.io": "^4.8.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "electron": "^33.2.1",
    "electron-builder": "^25.1.8",
    "fs-extra": "^11.3.2",
    "javascript-obfuscator": "^5.1.0",
    "nexe": "^4.0.0-rc.6"
  }
}